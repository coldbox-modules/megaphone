{
    "name":"megaphone",
    "version":"1.0.1",
    "author":"Ortus Solutions",
    "location":"forgeboxStorage",
    "homepage":"https://github.com/coldbox-modules/megaphone",
    "documentation":"https://megaphone.ortusbooks.com",
    "repository":{
        "type":"git",
        "URL":"https://github.com/coldbox-modules/megaphone"
    },
    "license":[
        {
            "type":"MIT",
            "URL":"https://github.com/coldbox-modules/megaphone/LICENSE"
        }
    ],
    "bugs":"https://github.com/coldbox-modules/megaphone/issues",
    "slug":"megaphone",
    "shortDescription":"A protocol-based library for sending Notifications in ColdBox",
    "description":"A protocol-based library for sending Notifications in ColdBox",
    "scripts":{
        "format":"cfformat run models/**/*.cfc,dsl/**/*.cfc,resources/**/*.cfc,tests/specs/**/*.cfc,tests/specs/**/*.cfc,tests/resources/**/*.cfc --overwrite",
        "format:check":"cfformat check models/**/*.cfc,dsl/**/*.cfc,resources/**/*.cfc,tests/specs/**/*.cfc,tests/specs/**/*.cfc,tests/resources/**/*.cfc --verbose",
        "format:watch":"cfformat watch models/**/*.cfc,dsl/**/*.cfc,resources/**/*.cfc,tests/specs/**/*.cfc,tests/specs/**/*.cfc,tests/resources/**/*.cfc",
        "install:cfpm":"cfpm install mysql,debugger",
        "onServerInitialInstall":"run-script install:cfpm"
    },
    "type":"modules",
    "dependencies":{
        "qb":"^9.4.0",
        "str":"^3.0.1"
    },
    "devDependencies":{
        "coldbox":"^6",
        "testbox":"^5.1.0+2",
        "cbmailservices":"^2.8.0+1",
        "hyper":"^7.2.1"
    },
    "installPaths":{
        "coldbox":"tests/resources/app/coldbox/",
        "testbox":"testbox/",
        "qb":"modules/qb/",
        "str":"modules/str/",
        "cbmailservices":"tests/resources/app/modules/cbmailservices/",
        "hyper":"tests/resources/app/modules/hyper/"
    },
    "ignore":[
        "**/.*",
        "test",
        "tests",
        "server.json"
    ]
}
